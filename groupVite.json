{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "description": "Group and schedule all vite related updates.",
    "packageRules": [
        {
            "groupName": "vite packages",
            "matchPackageNames": [
                "/@vitejs/plugin-react-swc/",
                "/vite/",
                "/vite-*/"
            ]
        }
    ]
}
